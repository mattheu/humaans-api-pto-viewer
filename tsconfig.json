{
	"compilerOptions": {
	  "target": "ES2019",
	  "strict": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "sourceMap": true,
	  "esModuleInterop": true,
	  "jsx": "react",
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "allowSyntheticDefaultImports": true,
	  "noImplicitReturns": true,
	  "noImplicitThis": true,
	  "noImplicitAny": true,
	  "strictNullChecks": true
	},
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Recommended"
  }
